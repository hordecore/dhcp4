import "logging"
import "matches"

option ms-classless-static-routes(249) = int[8]
option rfc3442-classless-static-routes(121) = int[8]
default-lease-time = max-lease-time = 172800
authoritative = true
log-facility = local7

if exists agent.circuit-id:
	log_raw()
	log_dlink()
	log_alter()
	log_snr()
	log_eltex_lte()

shared-network ISC:
	option dns-servers 8.8.8.8
	subnet 192.168.0.253/32:
	subnet 192.168.0.128/26:
	subnet 192.168.0.000/25:
		option routers 192.168.0.1

	### db bindings
	match_eltex_switch_ip(192.168.0.1):
		match_eltex_vlan(102) and match_eltex_port(2):
			range 192.168.0.35
		match_eltex_vlan(103) and match_eltex_port(3):
			range 192.168.0.36

	match_dlink_switch_ip(192.168.0.129):
		match_dlink_vlan(104) and match_dlink_port(4):
			range 192.168.0.37
		match_dlink_vlan(104) and match_dlink_port(5):
			range 192.168.0.38
